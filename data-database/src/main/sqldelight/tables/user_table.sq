CREATE TABLE User (
    id INTEGER PRIMARY KEY NOT NULL,
    username TEXT NOT NULL,
    name TEXT,
    image TEXT,
    email TEXT,
    lastWatchedAnimeDbModel TEXT
);

getUserFlow:
